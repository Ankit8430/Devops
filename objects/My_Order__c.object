    <validationRules>
        <fullName>Duplicacy</fullName>
        <active>true</active>
        <errorConditionFormula>AND(
isNew(),
Not( ISBLANK(VLOOKUP( $ObjectType.My_Product__c.Fields.Name,$ObjectType.My_Product__c.Fields.Name, Name )))
)</errorConditionFormula>
        <errorMessage>Duplicacy should be restricted</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>GST_num</fullName>
        <active>true</active>
        <errorConditionFormula>AND(   Total_Price__c &gt;2000, ISNULL(  GST_INN__c  ))</errorConditionFormula>
        <errorMessage>GST number should not blank</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Status_Error</fullName>
        <active>true</active>
        <errorConditionFormula>AND(ISCHANGED(   Status__c ), 
OR( 
ISPICKVAL(PRIORVALUE( Status__c  ),&quot;Delivered&quot;), 
ISPICKVAL(PRIORVALUE( Status__c  ),&quot;Delivered&quot;) 
) 
)</errorConditionFormula>
        <errorMessage>Can not change status after delivery.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Today_Date</fullName>
        <active>true</active>
        <errorConditionFormula>Date_of_Order__c  &lt; TODAY()</errorConditionFormula>
        <errorMessage>Please Enter correct date</errorMessage>
    </validationRules>
    <visibility>Public</visibility>
</CustomObject>
