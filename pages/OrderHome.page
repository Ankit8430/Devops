<apex:page controller="OrderHome">
    <apex:form >
        <apex:pageBlock title="Order Home Page">
            <apex:commandButton value="Home" action="{!home}"/>
            <apex:commandButton value="Referse" action="{!re}"/>
            <apex:image url="data:image/jpeg;base64,"/>
        <apex:pageMessages ></apex:pageMessages>
           <apex:pageBlockSection title="Add Order">
               <apex:commandButton value="Add Order" action="{!add}"/>
           </apex:pageBlockSection>
           <apex:pageBlockSection title="Edit Order">
               <apex:commandButton value="Edit Order" action="{!doEdit}"/>
           </apex:pageBlockSection>
            <apex:pageBlockSection title="Delete Order">
                <apex:commandButton value="Delete Order" action="{!doDetele}"/>
           </apex:pageBlockSection>
        </apex:pageBlock>
    </apex:form>
</apex:page>