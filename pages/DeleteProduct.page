<apex:page controller="DeleteProduct">
    <apex:form >
        <apex:pageBlock title="Product Delete">
            <apex:image url="https://thumbs.dreamstime.com/b/web-181534964.jpg" width="200px"/>
                     <apex:commandButton value="Product Home Page" action="{!product}"/>
                     <apex:commandButton value="Home Page" action="{!home}"/>
                     <apex:commandButton value="Referse" action="{!re}"/>
            <apex:pageBlockSection title="Product Details" columns="1">
            <apex:pageBlockTable value="{!lstWrap}" var="wrap">
            <apex:column >
            <apex:inputCheckbox value="{!wrap.isSelected}"/>
            </apex:column>
            <apex:column value="{!wrap.prodWrap.Name}"/>
            <apex:column value="{!wrap.prodWrap.Category__c}"/>
            <apex:column value="{!wrap.prodWrap.Category__c}"/>
            <apex:column value="{!wrap.prodWrap.Models__c}"/>
            <apex:column value="{!wrap.prodWrap.Cost__c}"/>
            <apex:column value="{!wrap.prodWrap.Approval_Status__c}"/>
            <apex:column value="{!wrap.prodWrap.Units_Available__c}"/>

            </apex:pageBlockTable>
            </apex:pageBlockSection>
            <apex:pageBlockButtons >
            <apex:commandButton value="Delete" action="{!doDelete}"/>
            </apex:pageBlockButtons>
        </apex:pageBlock>
    </apex:form>
</apex:page>