<apex:page controller="NewProduct">
    <apex:form >
            <apex:pageBlock title="Add Product">
                     <apex:commandButton value="Product Home Page" action="{!product}"/>
                    <apex:commandButton value="Home Page" action="{!home}"/>
                     <apex:commandButton value="Referse" action="{!re}"/>
                	<apex:image url="https://encrypted-tbn0.gstatic.com/images?q=tbn:ANd9GcRNqG8G9mSvcinF3ezdhszZkODvZHReCVnmtg&usqp=CAU"/>
                <apex:pageMessages ></apex:pageMessages>
                    <apex:pageBlockSection >
                    <apex:inputText value="{!prod.Name}"/>
                    <apex:inputField value="{!prod.Category__c}"/>
                    <apex:inputField value="{!prod.Brands__c}"/>
                    <apex:inputField value="{!prod.Models__c}"/>
                    <apex:inputField value="{!prod.Cost__c}"/>
                    <apex:inputField value="{!prod.Approval_Status__c}"/>
                    <apex:inputField value="{!prod.Units_Available__c}"/>
                    </apex:pageBlockSection>
                    <apex:pageBlockButtons >
                    <apex:commandButton value="Cancel" action="{!Cancel}"/>
                    <apex:commandButton value="save Product" action="{!saveProduct}"/>
                    </apex:pageBlockButtons>
            </apex:pageBlock>
    </apex:form>
</apex:page>